<Window x:Class="RayCaster.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RayCaster"
        xmlns:renderer="clr-namespace:RayCaster.Renderer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" SizeChanged="Window_SizeChanged" Loaded="Window_Loaded" MouseDoubleClick="Window_MouseDoubleClick" KeyDown="Window_KeyDown">
    <Grid x:Name="grid">
        <renderer:Display x:Name="display"/>
    </Grid>
</Window>
